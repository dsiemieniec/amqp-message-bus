{
    "name": "dsiemieniec/amqp-message-bus",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-pcntl": "*",
        "php-amqplib/php-amqplib": "^3.2",
        "symfony/console": "6.1.*",
        "psr/log": "^2.0.0",
        "symfony/property-access": "6.1.*",
        "symfony/serializer": "6.1.*",
        "symfony/dependency-injection": "^6.1",
        "symfony/config": "^6.1",
        "symfony/http-kernel": "^6.1",
        "symfony/framework-bundle": "^6.1"
    },
    "autoload": {
        "psr-4": {
            "Siemieniec\\AmqpMessageBus\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Siemieniec\\AmqpMessageBus\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "phpstan": "vendor/bin/phpstan analyse -c phpstan.neon --memory-limit=-1",
        "phpcs": "vendor/bin/phpcs -p",
        "phpcbf": "vendor/bin/phpcbf -p"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.8",
        "phpunit/phpunit": "^9.5",
        "slevomat/coding-standard": "^8.6",
        "squizlabs/php_codesniffer": "^3.7",
        "symfony/dotenv": "^6.1",
        "symfony/phpunit-bridge": "^6.1"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
